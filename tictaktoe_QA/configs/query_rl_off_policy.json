{
  "env_file": ".env",
  "dataset_source": "hf_hub",
  "hf_dataset_repo_id": "maxs-m87/tictactoe-qa-v1",
  "hf_dataset_revision": "main",
  "hf_cache_dir": "",
  "dataset_dir": "synth_dataset/outputs/v2",
  "train_split": "train",
  "val_split": "val",
  "final_eval_splits": [
    "val",
    "test",
    "benchmark_top50_canonical",
    "benchmark_top50_paraphrase"
  ],
  "finetune_id": "",
  "finetune_name": "",
  "rank": 16,
  "num_steps": 200,
  "resume_step": 0,
  "batch_size": 16,
  "group_size": 4,
  "lr": 0.0005,
  "temperature": 1.0,
