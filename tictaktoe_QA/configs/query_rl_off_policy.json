  "checkpoint_avg_splits": [
    "val",
    "test"
  ],
  "checkpoint_avg_metric": "eval_reward_mean",
  "checkpoint_ranking_output": "outputs/checkpoint_ranking_off_policy.json",
  "seed": 42,
  "best_metric": "eval_reward_mean",
